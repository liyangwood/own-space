{
  "name": "own-space",
  "version": "1.0.0",
  "description": "react+webpack+es6",
  "scripts": {
    "start": "npm run docker:start && rimraf dev && npm run build:dev_watch",
    "build:dev": "BABEL_ENV=development NODE_ENV=development webpack --config ./webpack/webpack.dev.js",
    "build:dev_watch": "BABEL_ENV=development NODE_ENV=development webpack --config ./webpack/webpack.dev.js --progress --watch",
    "docker:start": "docker-compose up -d",
    "clean": "rimraf dist",
    "build": "npm run clean && BABEL_ENV=production NODE_ENV=production webpack --config ./webpack/webpack.prod.js --progress",
    "lint": "eslint ./src/**/**.js",
    "test": "jest",
    "dockerhub": "npm run build && . ./deploy/build.sh"
  },
  "author": "liyangwood@gmail.com",
  "jest": {
    "setupFiles": [
      "<rootDir>/jest/boot.js"
    ],
    "mapCoverage": true,
    "collectCoverageFrom": [
      "app/**/*.{js,jsx,ts,tsx}"
    ],
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ]
  },
  "dependencies": {
    "bootstrap": "3.3.7",
    "cross-env": "^5.1.1",
    "lodash": "^4.17.4",
    "mailgun.js": "^2.0.1",
    "moment": "^2.19.1",
    "react": "16.0.0",
    "react-bootstrap": "^0.31.5",
    "react-dom": "16.0.0",
    "react-motion": "^0.5.2",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "4.1.1",
    "css-loader": "0.28.7",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.0",
    "eslint-plugin-react": "7.2.0",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "1.1.4",
    "html-webpack-plugin": "2.30.0",
    "jest": "21.2.0",
    "node-sass": "4.5.3",
    "open-browser-webpack-plugin": "0.0.5",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.0.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "url-loader": "0.6.1",
    "webpack": "3.8.0",
    "webpack-merge": "^4.1.0"
  }
}
